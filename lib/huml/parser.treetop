grammar Huml

  rule top
    space doctype:doctype? space html <Top>
  end

  rule block
    '%' name:[a-zA-Z1-9]+ space '{' space html space '}' space <Block>
  end

  rule doctype
    "doctype " [ \t]* typestring:([a-zA-Z1-9]*) <Doctype>
  end

  rule space
    [\s]* <Space>
  end

  rule html
    block* <HTML>
  end

end
